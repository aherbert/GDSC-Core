<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
  <suppress checks="RightCurly" files="(NullTrackProgressTest|TickerTest).java*" />
  <suppress checks="MissingJavadocType" files=".*[\\/]test[\\/].*" />

  <!-- LineLength is not a child of TreeWlker so cannot be suppressed by SuppressionCommentFilter. -->
  <suppress checks="LineLength" files="(CachedBicubicInterpolator|CustomTricubicInterpolator|CustomTricubicInterpolatingFunction).java" />
  <suppress checks="LineLength" files="(Double|Float)CustomTricubicFunction.java" />
</suppressions>
